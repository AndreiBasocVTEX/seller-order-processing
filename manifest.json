{
  "vendor": "elefant",
  "name": "seller-order-processing",
  "version": "1.0.11",
  "title": "Seller Order Processing",
  "description": "Order processing app with external carriers and invoicing services integrations",
  "builders": {
    "react": "3.x",
    "messages": "1.x",
    "docs": "0.x",
    "admin": "0.x",
    "node": "6.x"
  },
  "dependencies": {
    "vtex.styleguide": "9.x",
    "vtex.search-graphql": "0.x",
    "vtex.format-currency": "0.x"
  },
  "registries": ["smartcheckout"],
  "policies": [
    {
      "name": "template-criar"
    },
    {
      "name": "template-editar"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "urgentcargus.azure-api.net",
        "path": "/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.innoship.io",
        "path": "/api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.sameday.ro",
        "path": "/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.myvtex.com",
        "path": "/api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "www.selfawb.ro",
        "path": "/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "ws.smartbill.ro",
        "path": "/SBORO/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "/api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/*"
      }
    },
    {
      "name": "OMSViewer"
    }
  ],
